<%= form_for [@event, photo], html: { multipart: true } do |f| %>
  <%= render partial: 'partials/form_errors', locals: { model: photo } %>

  <div class="form-group">
   <strong><%= f.label :photo %></strong><br>
    <%= f.file_field :photo %>
  </div>

  <div class="actions">
    <%= f.submit t('.loaded'), class: 'btn btn-primary' %>
  </div>
<% end %>
